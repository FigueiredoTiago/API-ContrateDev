generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  githubId  Int      @unique
  login     String
  name      String?
  email     String?  @unique
  avatarUrl String?
  bio       String?
  location  String?
  website   String?
  githubUrl String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
